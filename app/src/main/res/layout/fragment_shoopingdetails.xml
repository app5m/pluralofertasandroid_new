<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#660066"
        app:layout_constraintBottom_toTopOf="@+id/guideline48"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/btnVoltarMeuPerfilTv"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="24dp"
            android:layout_marginStart="24dp"
            android:clickable="true"
            android:gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageButton
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:background="@drawable/backwardarrowbranca"
                android:clickable="false" />

        </LinearLayout>

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Detalhes da compra"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="@+id/btnVoltarMeuPerfilTv"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btnVoltarMeuPerfilTv" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline48"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/card_fundo_carrinho"
                    android:paddingStart="10dp"
                    android:paddingTop="23dp"

                    android:paddingEnd="10dp"
                    android:paddingBottom="23dp"
                    app:layout_constraintBottom_toTopOf="@+id/guideline87"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline93"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        app:layout_constraintGuide_percent="0.57" />

                    <TextView
                        android:id="@+id/textView33"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:text="ID:01"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/tituloCellMyCouponsEmAbertoTextView"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:ellipsize="end"
                        android:fontFamily="@font/uni_neue_heavy"
                        android:lineHeight="12dp"
                        android:lineSpacingExtra="10dp"
                        android:maxLines="3"
                        android:minLines="3"
                        android:text="Descrição da compra"
                        android:textAlignment="center"
                        android:textColor="#000000"
                        android:textSize="12dp"
                        app:layout_constraintBottom_toTopOf="@+id/guideline93"
                        app:layout_constraintEnd_toStartOf="@+id/guideline89"
                        app:layout_constraintStart_toStartOf="@+id/guideline90"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/validadeCellMyCouponsEmAbertoTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/uni_neue_regular"
                        android:lineHeight="24dp"
                        android:lineSpacingExtra="14sp"
                        android:text="Pedido feito em:"
                        android:textAlignment="textStart"
                        android:textColor="#000000"
                        android:textSize="12sp"
                        app:layout_constraintEnd_toStartOf="@+id/guideline89"
                        app:layout_constraintStart_toStartOf="@+id/guideline82"
                        app:layout_constraintTop_toTopOf="@+id/guideline93" />

                    <TextView
                        android:id="@+id/precoCellMyCouponsEmAbertoTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/uni_neue_bold"
                        android:gravity="end"
                        android:lineHeight="24dp"
                        android:lineSpacingExtra="12dp"
                        android:text="100,00"
                        android:textAlignment="textEnd"
                        android:textColor="#009933"
                        android:textSize="20dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/tituloCellMyCouponsEmAbertoTextView"
                        app:layout_constraintStart_toStartOf="@+id/textView39"
                        app:layout_constraintTop_toBottomOf="@+id/textView39" />

                    <TextView
                        android:id="@+id/rsCellMyCouponsEmAbertoTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/uni_neue_regular"
                        android:gravity="end"
                        android:lineHeight="24dp"
                        android:lineSpacingExtra="12dp"
                        android:text="R$"
                        android:textAlignment="textEnd"
                        android:textColor="#009933"
                        android:textSize="10dp"
                        app:layout_constraintBottom_toBottomOf="@+id/precoCellMyCouponsEmAbertoTextView"
                        app:layout_constraintEnd_toStartOf="@+id/precoCellMyCouponsEmAbertoTextView"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/precoCellMyCouponsEmAbertoTextView"
                        app:layout_constraintVertical_bias="1.0" />

                    <TextView
                        android:id="@+id/textView39"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="4dp"
                        android:background="@drawable/main_button_green"
                        android:text="Aprovada"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:visibility="visible"
                        app:layout_constraintEnd_toStartOf="@+id/guideline90"
                        app:layout_constraintStart_toStartOf="@+id/guideline91"
                        app:layout_constraintTop_toBottomOf="@+id/textView33" />

                    <TextView
                        android:id="@+id/textView35"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:text="12/06/2021"
                        app:layout_constraintEnd_toStartOf="@+id/guideline92"
                        app:layout_constraintTop_toTopOf="@+id/guideline93" />

                    <TextView
                        android:id="@+id/textView36"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:text="às"
                        app:layout_constraintBottom_toBottomOf="@+id/textView38"
                        app:layout_constraintEnd_toEndOf="@+id/validadeCellMyCouponsEmAbertoTextView"
                        app:layout_constraintTop_toBottomOf="@+id/validadeCellMyCouponsEmAbertoTextView" />

                    <TextView
                        android:id="@+id/textView38"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:text="14:00"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/textView35"
                        app:layout_constraintTop_toBottomOf="@+id/textView35" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline82"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.46" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline90"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.21" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline91"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.0" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline89"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.78" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline92"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="1.0" />


                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/productsShoopingDetailsRv"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginEnd="1dp"
                    android:visibility="visible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/guideline87" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideline87"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent="0.23" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>