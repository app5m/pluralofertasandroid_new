<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/card_fundo_carrinho"
    android:paddingStart="24dp"
    android:paddingTop="23dp"
    android:paddingEnd="35dp"

    >

    <TextView
        android:id="@+id/demandRsTv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/uni_neue_regular"
        android:gravity="end"
        android:lineHeight="24dp"
        android:lineSpacingExtra="12dp"
        android:text="R$"
        android:textAlignment="textEnd"
        android:textColor="#009933"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imagemCellMyCouponsEmAbertoImageView"
        app:layout_constraintStart_toEndOf="@+id/validadeCellMyCouponsEmAbertoTextView" />

    <TextView
        android:id="@+id/demandValueTv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/uni_neue_bold"
        android:gravity="end"
        android:lineHeight="24dp"
        android:lineSpacingExtra="12dp"
        android:text="100,00"
        android:textAlignment="textEnd"
        android:textColor="#009933"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/demandRsTv2"
        app:layout_constraintStart_toEndOf="@+id/demandRsTv2" />

    <ImageView
        android:id="@+id/imagemCellMyCouponsEmAbertoImageView"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:scaleType="centerCrop"
        android:src="@drawable/cupom_comprado_img"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/linhaCinzaCellMyCouponsEmAbertoLinearLayout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tituloCellMyCouponsEmAbertoTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="103dp"
        android:ellipsize="end"
        android:fontFamily="@font/uni_neue_heavy"
        android:lineHeight="12dp"
        android:lineSpacingExtra="10dp"
        android:maxLines="3"
        android:minLines="3"
        android:text="Crédito/Boleto"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline86"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/validadeCellMyCouponsEmAbertoTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/uni_neue_regular"
        android:lineHeight="24dp"
        android:lineSpacingExtra="14sp"
        android:text="Data  06/07/2021"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline86"
        app:layout_constraintTop_toBottomOf="@+id/tituloCellMyCouponsEmAbertoTextView" />

    <TextView
        android:id="@+id/validadeCellMyCouponsEmAbertoTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/uni_neue_regular"
        android:lineHeight="24dp"
        android:lineSpacingExtra="14sp"
        android:text="Às 14:30"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/validadeCellMyCouponsEmAbertoTextView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/validadeCellMyCouponsEmAbertoTextView"
        app:layout_constraintTop_toBottomOf="@+id/validadeCellMyCouponsEmAbertoTextView" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="96dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:background="@drawable/main_button_green"
        android:padding="6dp"
        android:text="Aprovado"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@+id/guideline94"
        app:layout_constraintTop_toTopOf="@+id/guideline88" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button_generate_tikect"
        android:padding="4dp"
        android:text="mostrar boleto"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/imagemCellMyCouponsEmAbertoImageView"
        app:layout_constraintEnd_toStartOf="@+id/guideline94" />

    <TextView
        android:id="@+id/waitPayment"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:fontFamily="@font/uni_neue_regular"
        android:lineSpacingExtra="4sp"
        android:text="@string/waitPayment"
        android:textAlignment="center"
        android:textColor="#ff5b5c"
        android:textSize="10sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@id/imagemCellMyCouponsEmAbertoImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/imagemCellMyCouponsEmAbertoImageView" />

    <LinearLayout
        android:id="@+id/linhaCinzaCellMyCouponsEmAbertoLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="23dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.406"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imagemCellMyCouponsEmAbertoImageView"
        app:layout_constraintVertical_bias="0.037">

    </LinearLayout>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline86"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline94"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="1.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline88"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline87"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.52" />


</androidx.constraintlayout.widget.ConstraintLayout>